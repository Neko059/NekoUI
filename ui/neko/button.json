  "fx_realms_button@common_buttons.light_text_button": {
    "$button_type_panel": "x_button_panel.realms_button_panel",
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "$border_visible": false,
    "size": [ "100%" , "100%" ],
    "$glyph_size": [ 15, 15 ],
    "$default_text_color|default": [ 1.0, 1.0, 1.0 ],
    "$hover_text_color|default": [ 1.0, 1.0, 1.0 ],
    "$pressed_text_color|default": [ 1.0, 1.0, 1.0 ],
    "$locked_text_color|default": [ 0.7, 0.7, 0.7 ]
  },

  "create_new_button_panel@common_buttons.new_ui_binding_button_label": {
    "layer": 10,
    "$anchor|default": "middle",
    "font_type": "MinecraftTen",
    "font_scale_factor": 1.1,
    "offset": [ 0 , -1.1 ] //  X , Y
  },

  "fx_create_new_button@common_buttons.light_text_button": {
    "$border_visible": false,
    "$button_type_panel": "x_button.create_new_button_panel",
    "size": [ "100%" , "100%" ],
    "$default_text_color|default": [ 0.0, 0.0, 0.0 ],
    "$hover_text_color|default": [ 0.81, 0.98, 1.00 ],
