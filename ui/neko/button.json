    "$pressed_button_texture|default": "textures/neko/main_ui_button/lightpressed",
    "$locked_button_texture|default": "textures/neko/main_ui_button/locked",
    
    "$default_text_color|default": [ 0.0, 0.0, 0.0 ],
    "$hover_text_color|default": [ 1.0, 1.0, 1.0 ],
    "$pressed_text_color|default": [ 1.0, 1.0, 1.0 ],
    "$locked_text_color|default": [ 0.5, 0.5, 0.5 ],

    "$glyph_texture": "textures/neko/main_icon/play_icon",
    "$pressed_button_name": "$play_button_target",
    "$button_text": "Play"
  },

  
  "test_code": {
    "$ukuran_tombol":  [ "100%" , "100%" ],
