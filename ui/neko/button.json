    "$locked_text_color|default": [ 0.3, 0.3, 0.3 ],
    "$default_button_texture|default": "textures/neko/light",
    "$hover_button_texture|default": "textures/neko/lighthover",
    "$pressed_button_texture|default": "textures/neko/lightpressed",
    "$locked_button_texture|default": "textures/neko/locked"
  },

  "play_button@common_buttons.light_text_button": {
    "size": [ "100%" , "100%" ],
    "$default_text_color|default": [ 0.0, 0.0, 0.0 ],
    "$hover_text_color|default": [ 1.0, 1.0, 1.0 ],
    "$pressed_text_color|default": [ 0.0, 0.0, 0.0 ],
    "$locked_text_color|default": [ 1.00, 0.26, 0.26 ],
