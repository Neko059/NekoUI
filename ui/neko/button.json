{
  "namespace": "x_button",

  "create_new_button_panel@common_buttons.new_ui_binding_button_label": {
    "layer": 10,
    "$anchor|default": "middle",
    "font_type": "MinecraftTen",
    "font_scale_factor": 1.1,
    "offset": [ 0 , -1.1 ] //  X , Y
  },

  ///////////////////
  // FXBUTTON 2    //
  ///////////////////

  "han_uwu": {
    "type": "panel",
    "$glyph_offset": [ 4 , 0 ],
    "$glyph_anchor_from": "left_middle",
    "$glyph_anchor_to": "left_middle",
    "$button_label_offset": [ 27 , -0.1 ],
    "layer": 5,
    "controls": [
      {
        "glyph": {
          "type": "image",
          "anchor_from": "$glyph_anchor_from",
          "anchor_to": "$glyph_anchor_to",
          "offset": "$glyph_offset",
          "texture": "$glyph_texture",
          "size": "$glyph_size",
          "layer": 6,
          "color": "$glyph_color",
          "alpha": "$content_alpha",

          "$glyph_bindings|default": [],
          "bindings": "$glyph_bindings"
        }
      },
      {
        "tab_button_text": {
          "type": "label",
          "font_type": "MinecraftTen",
          "anchor_from": "left_middle",
          "anchor_to": "left_middle",
          "font_scale_factor": 1.25,
          "text_alignment": "left",
          "size": [ "100% - 25px", "default" ],
          "max_size": [ "100% - 25px", "100% - 4px" ],
          "offset": [ 25 , -0.5 ],
          "text": "$button_text",
          "layer": 5,
          "color": "$text_color",
          "alpha": "$han_alpha",

          "$button_text_binding_type|default": "none",
          "$button_text_grid_collection_name|default": "",
          "$button_text_binding_condition|default": "none",

          "bindings": [
            {
              "binding_type": "$button_text_binding_type",
              "binding_condition": "$button_text_binding_condition",
              "binding_collection_name": "$button_text_grid_collection_name",
              "binding_name": "$button_text",
              "binding_name_override": "$button_text"
            }
          ]
        }
      }
    ]
  },


  "play_button_2@common_buttons.light_text_button": {
    "$glyph_size": [ 16, 16 ],
    "size": [ "100%" , "100%" ],
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "$button_type_panel": "x_button.han_uwu",
    "$border_visible": false,
    
    "$default_button_texture|default": "textures/neko/main_ui_button/light",
