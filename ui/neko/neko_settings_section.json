              }
            },
            {
              "@commonx.radio_with_label": {
                "size": [ "fill" , "30px" ],
                "$toggle_state_binding_name": "#thirdperson_radio_third_back",
                "$radio_label_text": "TPP"
              }
            },
            {
              "@commonx.radio_with_label": {
                "size": [ "fill" , "30px" ],
                "$toggle_state_binding_name": "#thirdperson_radio_third_front",
                "$radio_label_text": "TPF"
              }
            }
          ]
        }
      },
      { "spacer" :  {"type": "panel", "size": [ 6 , 6] }},
