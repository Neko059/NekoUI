  "gameplay_button@settings_common.section_toggle_base": {
    "$toggle_view_binding_name": "gameplay_button_toggle",
    "$glyph_texture": "textures/neko/glyph_icon/controller_glyph_color",
    "$glyph_color_texture": "textures/neko/glyph_icon/controller_glyph_color",
    "$glyph_size": [ 15, 15 ],
    "$button_text": "neko.gameplay",
    "$toggle_property_bag": {
      "#panel_title": "neko.title_gameplay"
    }
  },

  "gameplay_section": {
    "type": "stack_panel",
    "size": [ "100%", "100%c" ],
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "bindings": [
      {
        "binding_type": "view",
        "source_control_name": "gameplay_button_toggle",
        "source_property_name": "#toggle_state",
        "target_property_name": "#visible"
      }
    ],
    "controls": [
      { "@custom_settings_section.fxn_label_1": { "text": "Camera Perspective"}},
      { "spacer" :  {"type": "panel", "size": [ 5 , 5] }},
      {
        "perspective": {
          "type": "stack_panel",
          "size": [ "55%", "30px" ],
          "orientation": "horizontal",
          "controls": [
            {
              "@commonx.radio_with_label": {
                "size": [ "fill" , "30px" ],
                "$toggle_state_binding_name": "#thirdperson_radio_first",
